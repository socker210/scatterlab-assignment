{
  "name": "scatterlab-assignment",
  "version": "1.0.0",
  "description": "Scatter Lab assignment",
  "private": true,
  "author": "Junn",
  "license": "MIT",
  "keywords": [
    "scatterlab-assignment",
    "cat",
    "dog",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/socker210/scatterrab-assignment.git"
  },
  "bugs": {
    "url": "https://github.com/socker210/scatterrab-assignment/issues"
  },
  "scripts": {
    "dev": "pm2 start ecosystem.config.js",
    "kill": "pm2 delete ecosystem.config.js",
    "fix": "eslint . --ext .js --ext .jsx --fix",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "prod": "next build && pm2 start ecosystem.config.js --env production"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "express": "^4.17.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.14",
    "next": "^11.1.3",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.17",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-infinite-scroller": "^1.2.4"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-config-standard-react": "^8.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0"
  }
}
